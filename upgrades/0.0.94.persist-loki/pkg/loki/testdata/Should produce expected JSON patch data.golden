[
  {
    "op": "add",
    "path": "/config/schema_config/configs/-",
    "value": {
      "from": "2022-05-09",
      "store": "aws",
      "object_store": "s3",
      "schema": "v11",
      "index": {
        "prefix": "okctl-mock-cluster-loki-index_",
        "period": "336h"
      }
    }
  },
  {
    "op": "add",
    "path": "/config/storage_config/aws",
    "value": {
      "s3": "s3://eu-test-1",
      "bucketnames": "mock-bucket",
      "dynamodb": {
        "dynamodb_url": "dynamodb://eu-test-1"
      }
    }
  },
  {
    "op": "replace",
    "path": "/config/table_manager",
    "value": {
      "retention_deletes_enabled": true,
      "retention_period": "1344h",
      "index_tables_provisioning": {
        "provisioned_write_throughput": 1,
        "provisioned_read_throughput": 1,
        "inactive_write_throughput": 1,
        "inactive_read_throughput": 1
      }
    }
  },
  {
    "op": "replace",
    "path": "/serviceAccount",
    "value": {
      "create": false,
      "name": "loki",
      "annotations": {}
    }
  }
]